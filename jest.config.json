{
  "rootDir": "./src",
  "globals": {
    "window": true
  },
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "moduleNameMapper": {
    "\\.(css|jpg|png|svg|less)$": "<rootDir>/../node_modules/jest-css-modules",
    "nav-(.*)-style": "<rootDir>/../node_modules/jest-css-modules"
  },
  "collectCoverage": true,
  "coverageDirectory": "../coverage",
  "coveragePathIgnorePatterns": [
  ],
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 50,
      "lines": 50,
      "statements": 50
    }
  }
}
