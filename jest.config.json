{
  "collectCoverage": true,
  "coverageDirectory": "../coverage",
  "rootDir": "./src",
  "coveragePathIgnorePatterns": [
  ],
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 50,
      "lines": 50,
      "statements": 50
    }
  },
  "moduleNameMapper": {
    "^.+\\.(css|less|scss|sass)$": "babel-jest"
  }
}
