{
  "presets": [
    "@babel/typescript",
    "@babel/react",
    [
      "@babel/env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-arrow-functions",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}